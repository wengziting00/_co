/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or4Way(in=in[0..3], out=or4a_out);
    Or4Way(in=in[4..7], out=or4b_out);
    Or(a=or4a_out, b=or4b_out, out=out);
}
